{
  "name": "event-loop-animation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/event-loop-animation/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/event-loop-animation"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "event-loop-animation:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "event-loop-animation:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "event-loop-animation:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "event-loop-animation:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "event-loop-animation:build:development"
        },
        "production": {
          "buildTarget": "event-loop-animation:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/packages/event-loop-animation"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/event-loop-animation"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/event-loop-animation/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "event-loop-animation:build"
      }
    }
  },
  "tags": []
}
